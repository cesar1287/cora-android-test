<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolbarCora">
        <attr name="toolbarType" format="enum">
            <enum name="contacts" value="0" />
            <enum name="new_contact" value="1" />
        </attr>
        <attr name="titleToolbarCora" format="string" />
        <attr name="progressToolbarCora" format="integer" />
    </declare-styleable>

    <declare-styleable name="EditTextCora">
        <attr name="fieldType" format="enum">
            <enum name="none" value="0" />
            <enum name="cpf" value="1" />
            <enum name="name" value="2" />
            <enum name="number" value="3" />
        </attr>
        <attr name="maxLength" format="integer" />
        <attr name="placeholder" format="string" />
        <attr name="forcedPlaceholder" format="boolean" />
    </declare-styleable>
</resources>
